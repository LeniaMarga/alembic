{
  "name": "@openlab/alembic",
  "version": "0.1.3",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/*"
  ],
  "main": "dist/module.cjs",
  "module": "dist/module.js",
  "types": "dist/module.d.ts",
  "exports": {
    ".": {
      "types": "./dist/module.d.ts",
      "require": "./dist/module.cjs",
      "default": "./dist/module.js"
    },
    "./*": {
      "types": "./dist/module.d.ts",
      "require": "./dist/*.cjs",
      "import": "./dist/*.js",
      "default": "./dist/*"
    }
  },
  "scripts": {
    "build": "bin/build.sh"
  },
  "devDependencies": {
    "@11ty/eleventy": "^1.0.1",
    "@11ty/eleventy-plugin-syntaxhighlight": "^4.0.0",
    "@parcel/css-cli": "^1.5.0",
    "esbuild": "^0.14.24"
  }
}
