{
  "compilerOptions": {
    "outDir": "dist",
    "target": "ES2020",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "module": "ES2020",

    "strict": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "noEmitOnError": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "resolveJsonModule": true
  },
  "include": ["app.ts"]
}
