{
  "actions" : {
    "build" : {
      "enabled" : true,
      "postActions" : [
        {
          "action" : "build",
          "taskName" : "Development",
          "type" : "runTask"
        }
      ],
      "script" : "cd examples\/inject-styles\nnpm install --no-audit"
