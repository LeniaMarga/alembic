{
  "name": "@openlab/alembic",
  "version": "0.3.0-beta0",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/*"
  ],
  "main": "dist/module.js",
  "module": "dist/module.js",
  "types": "dist/module.d.ts",
  "exports": {
    ".": {
      "types": "./dist/module.d.ts",
      "require": "./dist/module.cjs",
      "default": "./dist/module.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "require": "./dist/*.cjs",
      "default": "./dist/*"
    }
  },
  "scripts": {
    "build": "./scripts/build-library.js && ./scripts/build-docs.sh",
    "test": "mocha"
  },
  "devDependencies": {
    "@11ty/eleventy": "^1.0.1",
    "@11ty/eleventy-plugin-syntaxhighlight": "^4.0.0",
    "@types/mocha": "^10.0.0",
    "@types/svgo": "^2.6.3",
    "dedent": "^0.7.0",
    "esbuild": "^0.16.11",
    "mocha": "^10.0.0",
    "prism-themes": "^1.9.0",
    "svgo": "^2.8.0",
    "ts-morph": "^17.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "markdown-it-anchor": "^8.6.6",
    "slugify": "^1.6.5"
  }
}
