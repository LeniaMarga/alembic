{
  "actions" : {
    "build" : {
      "enabled" : true,
      "postActions" : [
        {
          "action" : "build",
          "taskName" : "Development",
          "type" : "runTask"
        }
      ],
      "script" : "cd examples\/openlab-website\nmkdir -p dist\nnpm install --no-audit\nnpm run build"
    },
    "run" : {
      "enabled" : true,
      "script" : "cd examples\/openlab-website\nnpm run serve"
    }
  },
  "buildBeforeRunning" : true,
  "openLogOnRun" : "fail"
}
